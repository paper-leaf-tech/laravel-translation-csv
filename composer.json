{
  "name": "paper-leaf-tech/laravel-translation",
  "description": "Manage laravel translation strings using Google Sheets",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "spatie/laravel-package-tools": "^1.92",
    "google/apiclient": "^2.19"
  },
  "autoload": {
      "psr-4": {
          "PaperleafTech\\LaravelTranslation\\": "src"
      }
  },
  "authors": [
      {
          "name": "Brendan Angermna",
          "email": "brendan@paper-leaf.com"
      }
  ],
  "require-dev": {
    "laravel/pint": "^1.24"
  },
  "scripts": {
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PaperleafTech\\LaravelTranslation\\LaravelTranslationServiceProvider"
      ]
    },
    "google/apiclient-services": [
      "Sheets"
    ]
  }
}

